<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d_24"
        android:layout_marginTop="@dimen/d_32"
        android:src="@drawable/icons_menu_burger"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/d_24"
        android:src="@drawable/mask_group"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d_24"
        android:layout_marginTop="@dimen/d_24"
        android:layout_marginEnd="@dimen/d_24"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d_24"
        android:layout_marginTop="@dimen/d_30"
        android:text="Find "
        android:fontFamily="@font/lato_regular"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/d_24"
        app:layout_constraintEnd_toEndOf="@+id/editTextText"
        app:layout_constraintStart_toStartOf="@+id/editTextText"
        app:layout_constraintTop_toBottomOf="@+id/editTextText">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_consultation" />

        <TextView
            android:id="@+id/text1"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d_8"
            android:fontFamily="@font/lato_regular"
            android:text="Consultation"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_dental"
            tools:layout_editor_absoluteX="97dp"
            tools:layout_editor_absoluteY="194dp" />

        <TextView
            android:id="@+id/text2"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d_8"
            android:fontFamily="@font/lato_regular"
            android:text="Dental"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_heart"
            tools:layout_editor_absoluteX="209dp"
            tools:layout_editor_absoluteY="194dp" />

        <TextView
            android:id="@+id/text3"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d_8"
            android:fontFamily="@font/lato_regular"
            android:text="Heart"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_hospitals"
            tools:layout_editor_absoluteX="288dp"
            tools:layout_editor_absoluteY="182dp" />

        <TextView
            android:id="@+id/text4"
            android:layout_row="1"
            android:layout_column="3"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d_8"
            android:fontFamily="@font/lato_regular"
            android:text="Hospitals"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_medicines"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="281dp" />

        <TextView
            android:id="@+id/text5"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_gravity="center"
            android:fontFamily="@font/lato_regular"
            android:text="Medicines"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_physician"
            tools:layout_editor_absoluteX="107dp"
            tools:layout_editor_absoluteY="281dp" />

        <TextView
            android:id="@+id/text6"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_gravity="center"
            android:fontFamily="@font/lato_regular"
            android:text="Physician"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_skin"
            tools:layout_editor_absoluteX="209dp"
            tools:layout_editor_absoluteY="281dp" />

        <TextView
            android:id="@+id/text7"
            android:layout_row="3"
            android:layout_column="2"
            android:layout_gravity="center"
            android:fontFamily="@font/lato_regular"
            android:text="Skin"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_marginLeft="@dimen/d_23"
            android:layout_marginRight="@dimen/d_23"
            android:layout_marginBottom="@dimen/d_4"
            android:src="@drawable/ic_surgeon"
            tools:layout_editor_absoluteX="288dp"
            tools:layout_editor_absoluteY="281dp" />

        <TextView
            android:id="@+id/text8"
            android:layout_row="3"
            android:layout_column="3"
            android:layout_gravity="center"
            android:fontFamily="@font/lato_regular"
            android:text="Surgeon"
            android:textColor="@color/black"
            android:textSize="12sp" />

    </GridLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d_24"
        android:layout_marginTop="@dimen/d_24"
        android:fontFamily="@font/lato_regular"
        android:text="Top Doctors"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/d_24"
        android:fontFamily="@font/lato_regular"
        android:text="View all"
        android:textColor="@color/blue"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/d_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:listitem="@layout/recycler_view_row" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="your doctor"
        android:fontFamily="@font/lato_regular"
        android:textColor="@color/gray"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>